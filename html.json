{
  "break only pc": {
    "prefix": "onlypc",
    "body": [
      "class=\"only-pc\""
    ]
  },
  "break only sp": {
    "prefix": "only",
    "body": [
      "class=\"only-sp\""
    ]
  },
  "href blank": {
    "prefix": "blank",
    "body": [
      "rel=\"noreferrer\" target=\"_blank\""
    ]
  },
  "ejs function": {
    "prefix": "ejs",
    "body": [
      "<%",
      "const $1 = [",
      "  {",
      "    $2: 'xxx',",
      "  },",
      "];",
      "-%>",
      "<%-%>"
    ]
  },
  "php var_dump": {
    "prefix": "var",
    "body": [
      "<?php var_dump(); ?>"
    ]
  }
}
