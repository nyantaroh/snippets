{
  "border-radius": {
    "prefix": "rad",
    "body": [
      "border-radius: $0px;"
    ]
  },
  "nth-of-type": {
    "prefix": "nth",
    "body": [
      ":nth-of-type($0)"
    ]
  },
  "display block": {
    "prefix": "d",
    "body": [
      "display: block;"
    ]
  },
  "translate X": {
    "prefix": "tranx",
    "body": [
      "transform: translateX(${0:-50%});"
    ]
  },
  "translate Y": {
    "prefix": "trany",
    "body": [
      "transform: translateY(${0:-50%});"
    ]
  },
  "pseudo element": {
    "prefix": "giji",
    "body": [
      "position: absolute;",
      "top: 0;",
      "left: 0;",
      "width: ${0}px;",
      "height: ${1}px;",
      "background: url(/assets/images/${2}) no-repeat center / contain;",
      "content: \"\";"
    ]
  },
  "background": {
    "prefix": "bg",
    "body": [
      "background: #${0};"
    ]
  },
  "color": {
    "prefix": "c",
    "body": [
      "color: #${0};"
    ]
  },
  "outline": {
    "prefix": "out",
    "body": [
      "*, *::before, *::after {",
      "\toutline: 1px solid black;",
      "}"
    ]
  }
}
